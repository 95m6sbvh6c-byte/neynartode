{
  "version": 2,
  "public": true,
  "crons": [
    {
      "path": "/api/cron-notifications",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/announce-winner",
      "schedule": "*/10 * * * *"
    },
    {
      "path": "/api/finalize-contest",
      "schedule": "*/2 * * * *"
    }
  ],
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "app.html",
      "use": "@vercel/static"
    },
    {
      "src": ".well-known/farcaster.json",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/neynartode-sticker.png",
      "dest": "/public/neynartode-sticker.png"
    },
    {
      "src": "/Neynartodes_1200x638_splash.png",
      "dest": "/public/Neynartodes_1200x638_splash.png"
    },
    {
      "src": "/.well-known/farcaster.json",
      "dest": "/.well-known/farcaster.json"
    },
    {
      "src": "/",
      "dest": "/app.html"
    },
    {
      "src": "/frog",
      "dest": "/index.html"
    },
    {
      "src": "/app",
      "dest": "/app.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    }
  ],
  "env": {
    "NEYNAR_API_KEY": "AA2E0FC2-FDC0-466D-9EBA-4BCA968C9B1D"
  }
}
